<?xml version="1.0"?>
<td version="${TARGET_VERSION_MAJOR}.${TARGET_VERSION_MINOR}.${TARGET_VERSION_PATCH}">
  <TypeShield>
    <config>
      <name>Shield</name>
      <description>Defending system</description>
      <services>
        <parameter name="type">
          <value name="physical" description="Reduces self health to defending unit"/>
          <value name="energy" description="Created force field"/>
          <value name="gravity" description="Creates gravity field"/>
        </parameter>
        <parameter name="absorption">
          <min>0</min>
          <max>100</max>
        </parameter>
      </services>
    </config>
  </TypeShield>
</td>
