<?xml version="1.0"?>
<td version="${TARGET_VERSION_MAJOR}.${TARGET_VERSION_MINOR}.${TARGET_VERSION_PATCH}">
  <TypeCamera>
    <config>
      <name>Camera</name>
      <description>Displays world around</description>
      <services>
        <parameter name="range" description="Maximum view distance">
          <min>1</min>
          <max>65535</max>
        </parameter>
        <parameter name="type" description="Display data format">
          <value name="simple" description="Simplificity view"/>
          <value name="normal" description="Normal real view"/>
          <value name="night" description="Night vision"/>
          <value name="infrared" description="View in IR diapasone - displays temperature of objects"/>
          <value name="ultraviolet" description="View in UV diapasone - unvisible markers and other"/>
        </parameter>
        <parameter name="person" description="View person">
          <value name="1rd" description="From eyes"/>
          <value name="3rd" description="From back"/>
        </parameter>
        <parameter name="lookRangeX" description="Look range horizontal">
          <min>0</min>
          <max>360</max>
        </parameter>
        <parameter name="lookRangeY" description="Look range vertical">
          <min>0</min>
          <max>180</max>
        </parameter>
      </services>
    </config>
  </TypeCamera>
</td>
